{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["src/env.d.ts", "src/**/*", "src/**/*.vue", "./auto-imports.d.ts", "src/**/*.json"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "target": "esnext",
    "module": "esnext",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  }
}
